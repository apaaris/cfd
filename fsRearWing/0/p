FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

#include        "include/initialConditions"

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform $pressure;

boundaryField
{
    //- Set patchGroups for constraint patches
    #includeEtc "caseDicts/setConstraintTypes"

    inlet
    {
        type            zeroGradient;
    }

    outlet
    {
        type            fixedValue;
        value           $internalField;
    }

    ground
    {
        type            zeroGradient;
    }

    //motorBikeGroup
    //{
    //    type            zeroGradient;
    //}
    EndPlate_Right_patch
    {
        type            zeroGradient;
    }
    EndPlate_Left_patch
    {
        type            zeroGradient;
    }
    UpperWing_patch
    {
        type            zeroGradient;
    }
    LowerWing_patch
    {
        type            zeroGradient;
    }


    #include "include/frontBackUpperPatches"
}

